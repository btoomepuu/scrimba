{
  "extends": [
    "airbnb",
    "prettier",
    "plugin:node/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier", "jsx-a11y", "html"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": 2,
    "func-names": "off",
    "object-shorthand": "off",
    "no-plusplus": "off",
    "browser": "true",
    "singleQuote": true,
    "singleAttributePerLine": false
  }
}
